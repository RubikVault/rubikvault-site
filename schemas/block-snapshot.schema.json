{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Block Snapshot",
  "type": "object",
  "required": ["schemaVersion", "feature", "generatedAt", "data"],
  "properties": {
    "schemaVersion": { "type": "string" },
    "feature": { "type": "string" },
    "generatedAt": { "type": "string" },
    "meta": { "type": "object" },
    "data": {
      "type": "object",
      "required": ["items"],
      "properties": {
        "items": { "type": "array" }
      }
    }
  }
}
