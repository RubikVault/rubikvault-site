{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Forecast v6 Outcomes",
  "type": "object",
  "required": ["schema", "stream_revision", "rows"],
  "properties": {
    "schema": { "const": "forecast_outcomes_v6" },
    "stream_revision": { "type": "string", "pattern": "^v6\\.0-r\\d+$" },
    "rows": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["outcome_id", "prediction_id", "symbol", "asof_date", "outcome_date", "y", "outcome_policy_version", "algorithm_hash", "bars_manifest_hash", "input_hashes", "computed_at"],
        "properties": {
          "outcome_id": { "type": "string" },
          "prediction_id": { "type": "string" },
          "symbol": { "type": "string" },
          "asof_date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
          "outcome_date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
          "y": { "type": "integer", "enum": [0, 1] },
          "outcome_policy_version": { "type": "string" },
          "algorithm_hash": { "type": "string", "pattern": "^sha256:[a-f0-9]{64}$" },
          "bars_manifest_hash": { "type": "string", "pattern": "^sha256:[a-f0-9]{64}$" },
          "input_hashes": { "type": "object" },
          "computed_at": { "type": "string" }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
