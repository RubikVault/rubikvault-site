    "breadth.above_200dma": { "label":"% Above 200DMA","unit":"%","cadence":"daily","valueType":"number",
      "display":{"tooltip":"Share above 200-day moving average."},
      "thresholds":{"warnBelow":35,"alertBelow":25,"warnAbove":75,"alertAbove":85},
