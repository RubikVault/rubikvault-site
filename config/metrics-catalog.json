    },
    { "id": "fx_dollar", "title": "Major FX & Dollar", "order": 3,
      "metricIds": ["fx.dxy","fx.eurusd","fx.usdjpy","fx.gbpusd"]
    },
    { "id": "commodities", "title": "Commodities Snapshot", "order": 4,
      "metricIds": ["comm.gold","comm.wti","comm.brent","comm.copper"]
    },
    { "id": "market_closes", "title": "Market Closes", "order": 5,
