  "metricsCatalog": {
    "risk.vix": { "label":"VIX (Close)","unit":"index","cadence":"daily","valueType":"number",
      "display":{"tooltip":"Equity implied volatility index (close)."},
      "thresholds":{"warnAbove":25,"alertAbove":35},
      "sourceHint":{"primary":"FRED:VIXCLS"}
    },
    "risk.move": { "label":"MOVE Index (Bond Volatility)","unit":"index","cadence":"daily","valueType":"number",
      "display":{"tooltip":"Bond market volatility proxy."},
      "thresholds":{"warnAbove":120,"alertAbove":150},
      "sourceHint":{"primary":"ALT:MOVE (provider-specific)"}
    },
    "risk.regime": { "label":"Risk Regime","unit":"label","cadence":"daily","valueType":"label",
      "display":{"tooltip":"Derived label: RISK_ON / NEUTRAL / RISK_OFF."},
      "sourceHint":{"primary":"Derived"}
    },

    "rates.us10y": { "label":"US 10Y Treasury Yield","unit":"%","cadence":"daily","valueType":"number",
      "display":{"tooltip":"10-year Treasury yield."},
      "thresholds":{"warnAbove":5.0,"alertAbove":6.0},
      "sourceHint":{"primary":"FRED:DGS10"}
    },
    "rates.us2y": { "label":"US 2Y Treasury Yield","unit":"%","cadence":"daily","valueType":"number",
      "display":{"tooltip":"2-year Treasury yield."},
      "thresholds":{"warnAbove":5.5,"alertAbove":6.5},
      "sourceHint":{"primary":"FRED:DGS2"}
    },
    "rates.us30y": { "label":"US 30Y Treasury Yield","unit":"%","cadence":"daily","valueType":"number",
      "display":{"tooltip":"30-year Treasury yield."},
      "thresholds":{"warnAbove":6.0,"alertAbove":7.0},
      "sourceHint":{"primary":"FRED:DGS30"}
    },
    "rates.yield_curve": { "label":"Yield Curve (10Y-2Y)","unit":"bp","cadence":"daily","valueType":"number",
      "display":{"tooltip":"10Y minus 2Y spread. Negative = inversion."},
      "thresholds":{"warnBelow":-50,"alertBelow":-100},
      "sourceHint":{"primary":"Derived from DGS10-DGS2"}
    },
    "rates.sofr": { "label":"SOFR","unit":"%","cadence":"daily","valueType":"number",
      "display":{"tooltip":"Secured Overnight Financing Rate."},
      "sourceHint":{"primary":"FRED:SOFR"}
    },
    "rates.effr": { "label":"EFFR","unit":"%","cadence":"daily","valueType":"number",
      "display":{"tooltip":"Effective Federal Funds Rate."},
      "sourceHint":{"primary":"FRED:EFFR"}
    },

    "credit.hy_oas": { "label":"High Yield OAS","unit":"bp","cadence":"daily","valueType":"number",
