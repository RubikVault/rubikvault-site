    "credit.baa": { "label":"Moody's BAA Yield","unit":"%","cadence":"daily","valueType":"number",
      "display":{"tooltip":"BAA corporate bond yield."},
      "sourceHint":{"primary":"FRED:BAA"}
    },
    "credit.stress_spread": { "label":"Credit Stress (BAA-10Y)","unit":"bp","cadence":"daily","valueType":"number",
      "display":{"tooltip":"BAA yield minus 10Y yield. Higher = widening stress."},
      "thresholds":{"warnAbove":250,"alertAbove":350},
      "sourceHint":{"primary":"Derived from BAA and DGS10"}
    },

    "fx.dxy": { "label":"US Dollar Index (DXY)","unit":"index","cadence":"daily","valueType":"number",
