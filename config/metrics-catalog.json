        "encoding":"TOP|FLOP CSV",
        "format":"TOP:XLK:+5.2,XLF:+3.8|FLOP:XLE:-2.1,XLU:-1.7",
        "requiredEtfs":["XLK","XLF","XLE","XLV","XLI","XLY","XLP","XLU","XLC","XLRE","XLB"],
        "rankWindow":"5d_return_percent",
        "topN":2,
        "flopN":2
      },
      "sourceHint":{"primary":"Derived from sector ETF returns"}
    },

