{
  "schema": "providers.v1",
  "eod_chain": {
    "primary": "eodhd",
    "secondary": "twelvedata",
    "fallbacks": [
      "tiingo",
      "twelvedata"
    ],
    "defaults": {
      "outputsize": "260"
    }
  },
  "providers": {
    "tiingo": {
      "enabled": true,
      "type": "eod",
      "adapter": "tiingo"
    },
    "twelvedata": {
      "enabled": true,
      "type": "eod",
      "adapter": "twelvedata"
    },
    "eodhd": {
      "enabled": true,
      "type": "eod",
      "adapter": "eodhd"
    }
  }
}